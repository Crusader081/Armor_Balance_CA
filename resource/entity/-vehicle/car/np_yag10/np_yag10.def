{entity
	(include "/properties/car.ext")
    (include "/properties/l_car_mobility.ext")		
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{collider "truck"}
	{props "vehicle" "car" "truck" "wheeled" "vision_vehicle_minimum"}
	{able "emit_on_attack"}
	{targetClass "car" "fg bullet"}	
	("armor_light" args 1.0)

	("crew_2_human")
	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)
	("passenger_10_human")
	{extender "enumerator"}

	

  {boarder
    {anm "passenger"
      {forward  {begin  "board_body"} {end "seat_passenger"}}
      {reverse  {end  "emit_body" 1}}
    }
  }

	 (define "define_place"
	          {placer
	             {place "%place"
	                {linkbone "%place"}
	                {attach squat_lie}
	                {turnoff {shadow}}
	                {obstacle disable}
	             }
	          }
	          {Volume "%place"
	             {tags "attach" "attach_%place"}
	             {able clear {bullet 0} {touch 1}}
	          }
	)

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")
		
	{mass 5400}
	
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL3") boner("FXtraceR3"))
		("mobility"
			speed(45)
			weight(1.2)
			power(93)
			fuel(177)	
		)
	}
	
	{Extension "yag10.mdl"}
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 2} 
	}
	{Volume "bodyback"
		{tags "body"}
		{component "body"}
		{thickness 2}
	}
	{component "cabin"	
		{RepairTime 12}
	}
	{volume "cabin"
		{thickness 1}
	}
	{component "engine"	
		{RepairTime 10}
	}
	{volume "engine"
		{thickness 5}
	}
	

	
	
}
