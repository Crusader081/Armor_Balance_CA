{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc" scale(1.2))
	{props "medium" "-turret" "opened" "vision_vehicle_high" "muzzle_maximum"}
	
	{extender "enumerator"
		{digit_folder "ger"}
	}
	{extension "crew.ext"}

	{volume "sticky"
		{tags "grenade_fg_target"}
		{able clear {contact} {touch}}
		{component "sticky_body"}
	}
	
	{component "sticky_body"}
	{volume "body1"
		{component "body"}
		{tags "body"}
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
	}
	{volume "body3"
		{component "body"}
		{tags "body"}
	}
	{volume "body4"
		{component "body"}
		{tags "body"}
	}
	{volume "inventory"
        {thickness 2}
	}

	("armor_medium" body(50 15 15 15) turret(15 15 15 15))
	("armor_engine"  t(15 15 15 15))

	{extender "inventory"
		{box
            {item "ammo bullet150 fg" 30}
			{item "ammo bullet150 sm" 12}
		}
	}
