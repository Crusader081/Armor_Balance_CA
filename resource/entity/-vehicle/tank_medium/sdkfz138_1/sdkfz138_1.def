			{BrakeTime			1}
			{StopTime			2}
			{Gears				0.2 0.4 1}
			{RearGears			0.35}		
		}
		{FuelBag
			{volume	470 }	
			{rate	135 }   
			{remain	470 }
			{fuel	"fuel"}
		}
		{speed
        	{Normal	12}
        	{Slow	18}
    	}
		{DisableMoveWhileFiring}
	}
	{Extension "sdkfz138_1.mdl"}
	
	(mod "mp"
		{brain
			{properties
				{fire_mode hold}
			}
		}
	)
